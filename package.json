{
  "name": "node-iac",
  "version": "3.1.1",
  "main": "src/app.ts'",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/app.ts",
    "lint": "eslint src/*.ts",
    "prod": "node dist/index.js",
    "compile": "wireit",
    "build": "wireit",
    "docker": "docker buildx build  -t sirily11/etd-admin-node --platform linux/arm64,linux/amd64 --push . ",
    "test": "wireit",
    "it": "ts-node src/tests/integration_tests/run.ts",
    "release": "release-it --no-requireCleanWorkingDir"
  },
  "wireit": {
    "compile": {
      "command": "tsc"
    },
    "build": {
      "command": "ncc build build/app.js -o dist",
      "dependencies": [
        "compile"
      ]
    },
    "test": {
      "command": "jest --forceExit --coverage"
    }
  },
  "dependencies": {
    "@etherdata-blockchain/common": "^5.0.3",
    "@etherdata-blockchain/docker-plan": "^5.0.3",
    "@etherdata-blockchain/ip": "^5.0.3",
    "@etherdata-blockchain/logger": "^5.0.3",
    "@types/dockerode": "^3.3.21",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.7",
    "@types/node-cron": "^3.0.10",
    "@vercel/ncc": "^0.38.1",
    "axios": "^1.5.1",
    "axios-retry": "^3.8.0",
    "chalk": "^5.3.0",
    "dockerode": "^4.0.0",
    "dotenv": "^16.3.1",
    "http-method-enum": "^1.0.0",
    "http-status-codes": "^2.3.0",
    "internal-ip": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "node-cron": "^3.0.2",
    "typescript": "^5.2.2",
    "web3": "^4.2.0",
    "web3-eth-admin": "^1.0.0-beta.55",
    "wireit": "^0.14.1"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^7.0.2",
    "@types/chai": "^4.3.9",
    "@types/jest": "^29.5.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "chai": "^4.3.10",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nock": "^13.3.6",
    "prettier": "^3.0.3",
    "release-it": "^16.2.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "repository": "https://github.com/etherdata-blockchain/etd-local-admin-node.git",
  "author": "sirily11 <sirily1997@gmail.com>"
}