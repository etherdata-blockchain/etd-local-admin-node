{
  "name": "node-iac",
  "version": "3.1.1",
  "main": "src/app.ts'",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/app.ts",
    "lint": "eslint src/*.ts",
    "prod": "node dist/index.js",
    "compile": "wireit",
    "build": "wireit",
    "docker": "docker buildx build  -t sirily11/etd-admin-node --platform linux/arm64,linux/amd64 --push . ",
    "test": "wireit",
    "it": "ts-node src/tests/integration_tests/run.ts",
    "release": "release-it --no-requireCleanWorkingDir"
  },
  "wireit": {
    "compile": {
      "command": "tsc"
    },
    "build": {
      "command": "ncc build build/app.js -o dist",
      "dependencies": [
        "compile"
      ]
    },
    "test": {
      "command": "jest --forceExit --coverage"
    }
  },
  "dependencies": {
    "@etherdata-blockchain/common": "^5.0.2",
    "@etherdata-blockchain/docker-plan": "^5.0.2",
    "@etherdata-blockchain/ip": "^5.0.2",
    "@etherdata-blockchain/logger": "^5.0.2",
    "@types/dockerode": "^3.3.12",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.7",
    "@types/node-cron": "^3.0.5",
    "@vercel/ncc": "^0.34.0",
    "axios": "^1.1.3",
    "axios-retry": "^3.3.1",
    "chalk": "^5.1.2",
    "dockerode": "^3.3.4",
    "dotenv": "^16.0.3",
    "http-method-enum": "^1.0.0",
    "http-status-codes": "^2.2.0",
    "internal-ip": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "node-cron": "^3.0.2",
    "typescript": "^4.8.4",
    "web3": "^1.8.0",
    "web3-eth-admin": "^1.0.0-beta.55",
    "wireit": "^0.7.2"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/chai": "^4.3.3",
    "@types/jest": "^29.2.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "chai": "^4.3.6",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.2.2",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "release-it": "^15.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  },
  "repository": "https://github.com/etherdata-blockchain/etd-local-admin-node.git",
  "author": "sirily11 <sirily1997@gmail.com>"
}